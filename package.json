{
  "name": "xmlkit",
  "version": "0.3.0",
  "description": "HTMLparser2 toolkit for XML",
  "engine": {
    "node": ">=0.10 <0.12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lemonde/xmlkit"
  },
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha test",
    "coverage": "NODE_ENV=test node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "xml",
    "toolkit",
    "htmlparser2",
    "CSSselect"
  ],
  "author": "salper",
  "license": "MIT",
  "dependencies": {
    "CSSselect": "~0.7.0",
    "htmlparser2": "~3.7.1",
    "lodash": "~2.4.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "~1.9.1",
    "coveralls": "^2.10.0",
    "mocha": "^1.19.0",
    "mocha-lcov-reporter": "0.0.1",
    "underscore.string": "^2.3.3"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  }
}
